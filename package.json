{
    "name": "com.zemil.mobile",
    "displayName": "zmobile",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "scripts": {
        "dev": "webpack-dev-server --config js-app/webpack.dev.js --mode development --open",
        "build": "webpack --config js-app/webpack.build.js --mode production",
        "android:check": "cordova requirements android",
        "android:build": "cordova build android --verbose",
        "android:run": "cordova run android"
    },
    "author": "zemil",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-android": "7.0.0",
        "cordova-plugin-whitelist": "^1.3.3",
        "phaser": "^3.11.0"
    },
    "devDependencies": {
        "babel-core": "^6.9.1",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-stage-1": "^6.5.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^0.28.11",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.9.0",
        "postcss-loader": "^2.1.5",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "webpack": "^4.12.1",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.4"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "android"
        ]
    }
}